+void ByteArray::acquireBuffer(uint8_t* buf, int bufLen)
+{
+	if(bytes)
+	{
+#ifdef MEMORY_USAGE_PROFILING
+		getClass()->memoryAccount->removeBytes(real_len);
+#endif
+		free(bytes);
+	}
+	bytes=buf;
+	real_len=bufLen;
+	len=bufLen;
+#ifdef MEMORY_USAGE_PROFILING
+	getClass()->memoryAccount->addBytes(real_len);
+#endif
+	position=0;
+}
