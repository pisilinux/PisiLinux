+tiny_string tiny_string::fromChar(uint32_t c)
+{
+	tiny_string ret;
+	ret.buf = ret._buf_static;
+	ret.type = STATIC;
+	ret.stringSize = g_unichar_to_utf8(c,ret.buf) + 1;
+	ret.buf[ret.stringSize-1] = '\0';
+	return ret;
+}
