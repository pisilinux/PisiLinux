+void ByteArray::writeU29(uint32_t val)
+{
+	for(uint32_t i=0;i<4;i++)
+	{
+		uint8_t b;
+		if(i<3)
+		{
+			uint32_t tmp=(val >> ((3-i)*7));
+			if(tmp==0)
+				continue;
+
+			b=(tmp&0x7f)|0x80;
+		}
+		else
+			b=val&0x7f;
+
+		writeByte(b);
+	}
+}
