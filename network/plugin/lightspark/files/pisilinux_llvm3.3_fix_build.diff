+void tiny_string::makePrivateCopy(const char* s)
+{
+	resetToStatic();
+	stringSize=strlen(s)+1;
+	if(stringSize > STATIC_SIZE)
+		createBuffer(stringSize);
+	strcpy(buf,s);
