+bool ByteArray::readU29(uint32_t& ret)
+{
+	//Be careful! This is different from u32 parsing.
+	//Here the most significant bits appears before in the stream!
+	ret=0;
+	for(uint32_t i=0;i<4;i++)
+	{
+		if (len <= position)
+			return false;
+
+		uint8_t tmp=bytes[position++];
+		ret <<= 7;
+		if(i<3)
+		{
+			ret |= tmp&0x7f;
+			if((tmp&0x80)==0)
+				break;
+		}
+		else
+		{
+			ret |= tmp;
+			//Sign extend
+			if(tmp&0x80)
+				ret|=0xe0000000;
+		}
+	}
+	return true;
+}
