+			set_sense_type(chip, lun, SENSE_TYPE_MEDIA_INVALID_CMD_FIELD);
+			TRACE_RET(chip, TRANSPORT_FAILED);
+		}
+		break;
+		
+	case KF_SET_ICV:
+		ms_card->mg_entry_num = srb->cmnd[5];
+		if ((scsi_bufflen(srb) == 0x404) &&
+			(srb->cmnd[8] == 0x04) &&
+			(srb->cmnd[9] == 0x04) && 
+			(srb->cmnd[2] == 0x00) && 
+			(srb->cmnd[3] == 0x00) && 
+			(srb->cmnd[4] == 0x00) && 
+			(srb->cmnd[5] < 32)) 
