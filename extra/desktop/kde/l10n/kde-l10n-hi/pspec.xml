        <IsA>locale:hi</IsA>
        <Summary>Hindi KDE4 internationalization package</Summary>
        <Description>kde-l10n-hi is the KDE4 internationalization package that provides Hindi translations for KDE4 applications.</Description>
        <Archive type="tarxz" sha1sum="ec97ca9fecbfc3a6335c9b1b989c82bf03880fc4">mirrors://kde/stable/4.14.3/src/kde-l10n/kde-l10n-hi-4.14.3.tar.xz</Archive>
        <BuildDependencies>
            <Dependency>kdelibs-devel</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>kde-l10n-hi</Name>
        <Icon>lang-hi</Icon>
        <PartOf>system.locale</PartOf>
        <Replaces>
            <Package>kde4-i18n-hi</Package>
            <Package>kde-i18n-hi</Package>
        </Replaces>
        <Files>
            <Path fileType="localedata">/usr/share/locale</Path>
            <Path fileType="data">/usr/share/kde4</Path>
            <Path fileType="doc">/usr/share/doc</Path>
            <Path fileType="man">/usr/share/man</Path>
        </Files>
    </Package>

