        </Patches>
    </Source>

    <Package>
        <Name>cups-filters</Name>
        <RuntimeDependencies>
            <Dependency>cups</Dependency>
            <Dependency versionFrom="5.0.0">qpdf</Dependency>
            <Dependency>tiff</Dependency>
            <Dependency>lcms2</Dependency>
